{
	"env": {
		"browser": true,
		"es2021": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"ecmaFeatures": {
			"impliedStrict": true
		}
	},
	"rules": {
		"no-inner-declarations": [
			2,
			"both"
		],
		"no-useless-computed-key": 2,
		"no-var": 0,
		"prefer-arrow-callback": 0,
		"comma-dangle": 2
	}
}
